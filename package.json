{
  "name": "@dgiot/dgiot-dashboard",
  "version": "4.3.8",
  "author": "iotn2n",
  "license": "Mozilla Public License Version 2.0",
  "private": false,
  "homepage": "https://github.com/dgiot/dgiot-dashboard.git",
  "scripts": {
    "dev": "vue-cli-service serve",
    "start": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "dgiot:install": "start ./install.sh",
    "build": "vue-cli-service build --report && npm run cp && rimraf dist/example/",
    "lint": "eslint --ext .js --fix&&vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "lint:style": "stylelint **/*.{vue,scss} --fix",
    "image-webpack-loader": "cnpm install --save-dev image-webpack-loader",
    "inspect": "vue-cli-service inspect",
    "ncu": "npm run global add npm-check-updates",
    "swagger": "rimraf swagger.json && node src/utils/swagger/index.js",
    "update": "ncu -u --reject eslint,sass-loader,compression-webpack-plugin,filemanager-webpack-plugin --registry https://registry.npmmirror.com&&npm install --registry=https://registry.npmmirror.com",
    "changelog:zh": "rimraf CHANGELOG.ZH.md && conventional-changelog -p angular -i CHANGELOG.ZH.md -r 0 -s",
    "changelog:en": "rimraf CHANGELOG.EN.md && conventional-changelog -p angular -i CHANGELOG.EN.md -r 0 -s",
    "clear": "npm cache clean -f && rimraf node_modules",
    "clear:install": "npm install -g rimraf && npm run clear && cnpm install",
    "checkout:main": "git checkout master && git  pull origin master && git checkout main &&  git pull origin main &&  rimraf .git && mkdir .git && cp -r .git/* .git && if exist .git echo '.git'",
    "use:pmpm": "npm i -g pnpm --registry=https://registry.npmmirror.com",
    "use:github": "rimraf .git/config && rimraf .git && mkdir .git && cp -r .git/* .git && npm run remoteinfo && npm run copy:config  && git pull origin master &&  npm run changelog:zh && npm run changelog:en && git add . && git cz",
    "use:github:main": "rimraf .git/config && rimraf .git && mkdir .git && cp -r .git/* .git && npm run remoteinfo && npm run copy:config  && git pull origin main &&  npm run changelog:zh && npm run changelog:en && git add . && git cz",
    "use:gitbit": "rimraf .git/config && rimraf .git && mkdir .git && cp -r gitbit/* .git && npm run remoteinfo && git pull origin master && npm run changelog:zh && npm run changelog:en  && git add . && git cz",
    "remoteinfo": "git remote -v",
    "genSwagger": "node src/utils/genSwagger.js",
    "set:github": "npm run remoteinfo && git push origin master && rimraf .git && mkdir .git && cp -r .git/* .git && if exist .git echo '.git'",
    "push": "npm run use:github && npm run set:github",
    "set:github:main": "npm run remoteinfo && git push origin main && rimraf .git && mkdir .git && cp -r gitbit .git && if exist .git echo '.git'",
    "set:gitbit": "npm run remoteinfo && git push origin master && rimraf gitbit && mkdir gitbit && cp -r gitbit gitbit && if exist gitbit echo 'gitbit'",
    "copy:config": "rimraf src/config/developer.config.js && cp src/config/developer.lock.js src/config/developer.config.js && echo 'copy config done'",
    "downCdn": "node utils/downCdn.js",
    "downIconfront": "node utils/downIconfront.js",
    "cp": "node utils/cp.js",
    "changelog": "standard-version 0.0.1 && pnpm changelog:zh && pnpm changelog:en",
    "cp:git": "rimraf .git && cp -r .git .git",
    "rmEnv": "rimraf .env",
    "local:server": "node utils/server.js",
    "lighthouse": "node utils/lighthouse.js",
    "prod": "lighthouse https://prod.iotn2n.com/",
    "template": "plop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgiot/dgiot-dashboard.git"
  },
  "dependencies": {
    "@babel/helper-plugin-utils": "7.14.5",
    "@dgiot/dgiot-component": "0.0.3",
    "@dgiot/dgiot-mqtt-dashboard": "1.1.5",
    "@material/icon-button": "13.0.0",
    "@vue/babel-helper-vue-jsx-merge-props": "1.2.1",
    "@vue/cli-plugin-pwa": "^4.5.14",
    "amis": "1.4.1",
    "axios": "0.24.0",
    "clean-webpack-plugin": "4.0.0",
    "clone": "^2.1.2",
    "conventional-changelog-cli": "^2.1.1",
    "element-ui": "2.15.6",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.4.0",
    "html-webpack-plugin": "5.5.0",
    "indent-string": "5.0.0",
    "ip": "1.1.5",
    "konva": "8.2.3",
    "logrocket": "^2.1.1",
    "logrocket-vuex": "^0.0.3",
    "monaco-editor-webpack-plugin": "6.0.0",
    "plop": "^2.7.6",
    "q": "^1.5.1",
    "qs": "6.10.1",
    "react-dom": "^16.14.0",
    "register-service-worker": "1.7.2",
    "resize-detector": "0.3.0",
    "standard-version": "^9.3.2",
    "vab-icons": "git+https://github.com.cnpmjs.org/zxcasvw/vab-icons.git",
    "vab-templates": "^0.0.5",
    "vue-contextmenu": "1.5.10",
    "vue-ele-form-data-editor": "0.1.6",
    "vue-ele-form-dynamic": "0.4.3",
    "vuera": "^0.2.7"
  },
  "devDependencies": {
    "@babel/runtime": "7.16.3",
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@vue/cli-plugin-babel": "^4.5.14",
    "@vue/cli-plugin-eslint": "^4.5.14",
    "@vue/cli-plugin-router": "^4.5.14",
    "@vue/cli-plugin-vuex": "^4.5.14",
    "@vue/cli-service": "^4.5.14",
    "@vue/eslint-config-prettier": "6.0.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "body-parser": "1.19.0",
    "chalk": "4.1.2",
    "chokidar": "3.5.2",
    "chrome-launcher": "0.15.0",
    "commitizen": "4.2.4",
    "compression-webpack-plugin": "^9.0.0",
    "copy-to-clipboard": "3.3.1",
    "core-js": "3.19.1",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "6.3.0",
    "eslint": "^8.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.0.3",
    "filemanager-webpack-plugin": "6.1.7",
    "hard-source-webpack-plugin": "0.13.1",
    "husky": "7.0.4",
    "lighthouse": "8.6.0",
    "lint-staged": "11.2.6",
    "mini-css-extract-plugin": "2.4.4",
    "monaco-editor": "0.30.1",
    "prettier": "2.4.1",
    "pushstate-server": "3.1.0",
    "raw-loader": "4.0.2",
    "rimraf": "3.0.2",
    "sass": "~1.43.4",
    "sass-loader": "^12.3.0",
    "script-loader": "0.7.2",
    "shelljs": "0.8.4",
    "speed-measure-webpack-plugin": "1.5.0",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "svg-sprite-loader": "6.0.11",
    "url-loader": "4.1.1",
    "vue-eslint-parser": "8.0.1",
    "vue-template-compiler": "2.6.14",
    "webpackbar": "5.0.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint"
    ]
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 3.0.0"
  }
}
